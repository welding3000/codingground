// --------------------------------------------
$green: #a1c057; // important  
$white: white;   // important
$black: black;   // important
$Hblack: #333;   // important
$line:  inline;  // important
$none:  none;    // important
$over:  auto;    // important
$dark:  1px solid #333; // important 
$aqua:  1px solid aqua; // important
$tp:    15px;      // important
$pd:    15px 15px; // important
$twen:  20px;      // important
$darkblue: rgba(33, 34, 66, 0.77); // important  
// --------------------------------------------
$rel:  relative; // important
$abs:  absolute; // important
$flex: flex ;    // important
$ct:   center;   // important
$per:  900px;    // important
$jt:   justify;  // important
$rpt:  no-repeat ;    // important
$ard:  space-around;  // important
$bet:  space-between; // important
$rob:  'Roboto', sans-sefif; // important
$shadow: 0px 20px 100px #555; // important
$back:   linear-gradient(to bottom, #59476f 30%, #7b88d1 100%); // important
$ground: linear-gradient(to top, #eba65b 30%, #d99267 100%);    // important
                                                            
/* ::::::::::::----------------------------::::::::::::: */
@mixin bg-black() { background: $black;   }      // background 
@mixin bg-Hblack(){ background: $Hblack;  }      // background 
@mixin bg-green() { background: $green;   }      // background 
@mixin bg-darkblue(){ background: $darkblue; }   // background 
@mixin bg-rpt()   { background-repeat: $rpt; }   // background 
@mixin cl-white() {  color:  $white;      }      // color
@mixin br-aqua()  {  border: $aqua;       }      // border
@mixin br-dark()  {  border: $dark;       }      // border
@mixin overflow() {  overflow: $over;     }      // overflow
@mixin over-x()   {  overflow-x: $over;   }      // overflow
@mixin pd()       {  padding: $pd;        }      // padding
@mixin pd-twen()  {  padding: $twen;      }      // padding
@mixin mar-tp()   {  margin-top: $tp;     }      // margin
@mixin mar-over() {  margin: $over;       }      // margin
@mixin ds-ln()    {  display: $line;      }      // display 
@mixin ds-no()    {  display: $none;      }      // display 
@mixin ds-fx()    {  display: $flex;      }      // display
@mixin tx-dec()   {  text-decoration: $none; }   // text-decoration
@mixin tx-al()    {  text-align: $ct;     }      // list style
@mixin lt-st()    {  list-style: $none;   }      // list style
@mixin pt-abs()   {  position:$abs;       }      // position
@mixin pt-rel()   {  position:$rel;       }      // position   
@mixin font()     {  font-family: $rob ;     }   // font
@mixin js-bet()   {  justify-content: $bet;  }   // justify
@mixin js-ard()   {  justify-content: $ard;  }   // justify
@mixin al-sf()    {  align-self: $ct ;    }      // align 
@mixin per()      {  perspective: $per;   }      // pers
/* ::::::::::::----------------------------::::::::::::: */
/* ::::::::::::-----------nav-blue-header-----------------::::::::::::: */
// position:$rel;  display: $flex;  font-family: $rob ;  color: $white;
@mixin header() { @include pt-rel(); @include ds-fx(); @include font(); @include cl-white();}

//display: $flex;  justify-content: $bet;
@mixin hd-nav() { @include ds-fx(); @include js-bet();}

// display: $flex;  justify-content: $ard;
@mixin bx-menu() { @include ds-fx(); @include js-ard();}

//  align-self: $ct;  display: $flex;
@mixin bx-search() { @include al-sf(); @include ds-fx();}

//text-decoration: $none;  color: $white;
@mixin add-a() { @include tx-dec(); @include cl-white();}
/* ::::::::::::-----------nav-blue-header-----------------::::::::::::: */

/* ::::::::::::-----------visual-text-print-----------------::::::::::::: */
//  margin: $over; 
@mixin body() {    @include mar-over() ;}

// background: $black; color: $white;  border: $aqua;
@mixin acordionBox() {  @include bg-black() ;  @include cl-white() ;  @include br-aqua() ;  }

// overflow: $over;   border: $aqua;  background: $Hblack;
@mixin textAcordion() { @include overflow() ;  @include br-aqua() ;  @include bg-Hblack() ;}

// color: $white;   border: $aqua;   overflow: $over;
@mixin caja() { @include cl-white() ;  @include br-aqua() ; @include overflow() ; }

// list-style: $none;   background: $green;  padding: $twen;  
@mixin ul() {   @include lt-st() ;   @include bg-green() ;    @include pd-twen() ; }

//  border: $aqua; 
@mixin hide-ul() { @include br-aqua(); }

//  display: $line; 
@mixin li() { @include ds-ln(); }

//  text-decoration: $none;   color: $white;
@mixin a() { @include tx-dec() ;    @include cl-white() ;}

//  border: $dark;  background: $Hblack; 
@mixin hide-a() { @include br-dark() ;   @include bg-Hblack() ; }

//  overflow: $over;   border: $aqua;   display: $none; 
@mixin effect-code() { @include overflow() ;   @include br-aqua() ;   @include ds-no() ; }

//  background: rgba(33, 34, 66, 0.77);   color: $white;  overflow: $over;   display: $none; 
@mixin effect-output() { @include darkblue() ;   @include cl-white() ;   @include overflow() ;   @include ds-no() ;   }

//  color: $white;   overflow: $over;  background: rgba(33, 34, 66, 0.77);  display: $none;
@mixin effect-print() { @include cl-white() ;   @include overflow() ;   @include bg-darkblue() ;   @include ds-no() ;}

//  margin-top: $tp; padding: $pd; overflow-x: $over;  color: $white;
@mixin boxout() { @include mar-tp() ;   @include pd() ;   @include over-x() ;   @include cl-white() ; }

//  margin-top: $tp; padding: $pd;  overflow: $over; 
@mixin box() { @include mar-tp() ;   @include pd() ;   @include overflow() ; }

// color: $white; border: $aqua;  overflow-x: $over; 
@mixin tabs() {  @include cl-white() ;   @include br-aqua() ;   @include over-x() ; }
