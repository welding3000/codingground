/*  Fondo y estilo de letras */
@import url('https://fonts.googleapis.com/css?family=Raleway:200');
body {
  font-family: 'Raleway', sans-serif;
  background: url('http://imagenesdepaisajesnaturales.net/wp-content/uploads/2016/08/fondos-de-pantalla-de-playas-transparente.jpg');
  background-repeat: no-repeat;
  background-size: 100% 20%;
  // background: rgba(161,192,87,0.9 );
}

@mixin model($tp,$lft,$wd,$he) {
  top: $tp;  left: $lft;  width: $wd;  height: $he;
}

@mixin wrapper() {
  position: $abs; // important
  //  10% , 40%  , 300px ,  500px 
  @include model( 80px,
  10px,
  380px,
  1200px); //  top , left , width ,  height 
}

.wrapper {
  @include wrapper();
  perspective: 900px;
  background: rgba(161, 192, 87, 0.9);
}

// produce los effectos de animaccion 
@mixin container($tp,$wd,$he) {
  top: $tp;  width: $wd;  height: $he;  position: $abs;
}

.container {
  transition: .5s all ease;
  transform: rotateX(40deg) scale(0.4); // 
  perspective: 900px;
  box-shadow: 0px 20px 50px #555;
  animation: entry 1s linear 1;
  @include container( 20%,
  100%,
  10%); //   top , width ,  height 
}

// color y posision primer box
//@mixin c0($tp ,  $wd , $he , $in  ,$rad ) {    top: $tp ;  width: $wd;  height: $he;  z-index: $in;   position: $post;   border-radius:$rad ; }
#c0 {
  background: rgba(19, 80, 109, 0.8);
  // :::::::::::  top , width ,  height  , index ,radius
  //  @include c0(   0%  , 300px ,  500px    , 300  , 21px  );
  position: $abs;
  width: 800px;
  height: 270px;
  left: -210px;
  top: -60px;
}

// posision del 2 box
@mixin c1( $wd,$he,$tp,$lt,$in,$rad) {
  width: $wd;  height: $he;  top: $tp;  left: $lt;  z-index: $in;  border-radius: $rad;
}

#c1 {
  // background: linear-gradient(to top right, #3f51b5 0%, #039be5 40%);
  background: rgba(133, 21, 21, 0.8);
  // 800px,270px, 23% , -180%, 10 , 21px  
  @include c1( 800px,
  270px,
  55px,
  -210px,
  10,
  0px); //  width| height| top| left| index |radius
}
// posision del 3 box  izquierda 
@mixin c2( $wd,$he,$tp,$lt,$in,$rad) {
  width: $wd;  height: $he;  top: $tp;  left: $lt;  z-index: $in;  border-radius: $rad;
}
#c2 {
  background: rgba(33, 33, 33, 0.8);
  //  800px , 270px , 46%, -180% ,   2   , 21px   
  @include c2( 800px,
  270px,
  175px,
  -210px,
  1,
  0px); //  width| height| top| left| index |radius
}
// posision del 4 box  
@mixin c3($wd,$he,$tp,$lt,$in,$rad) {
  width: $wd;  height: $he;  top: $tp;  left: $lt;  z-index: $in;  border-radius: $rad;
}
#c3 {
  background: rgba(61, 51, 221, 0.8);
  // 800px , 270px , 69% , -180%  , 0     , 21px 
  @include c3( 800px,
  270px,
  295px,
  -210px,
  0,
  0px); //  width| height| top| left| index |radius
}
/* Effecto con el mouse */
.container:hover {
  cursor: pointer;
  transform: rotate(0deg) scale(0.5) translateY(10px); // 1.5
  transition: .5s all ease;
  z-index: 400;
}
// Establecer imagen principal
@mixin image($tp,$lft,$wd,$he) {
  top: $tp;  left: $lft;  width: $wd;  height: $he;  position: $abs;}
.image {
  border-radius: 21px;
  background-image: url('https://codigoespagueti.com/wp-content/uploads/2016/02/CrashBandicoot.jpg');
  opacity: 0.8;
  background-repeat: no-repeat;
  // imagen navegador
  background-size: 300px 350px;
  //  0% ,  0%  , 100% ,  100%    
  @include image( 0%,  0%,  500px,  270px); // top , left , width ,  height 
}
// Este da el color de la parte de abajo de una imagen para seleccionar todos y las words
@mixin story($tp,$lft,$wd,$he,$in) {
  top: $tp;  left: $lft;  width: $wd;  height: $he;  z-index: $in;  position: $abs;
}
.story {
  //  5% , -1%  ,  100%  , 55%   ,30 
  @include story( 5px,  -1px,  800px,  85px,  30); // top , left , width ,  height , index   
}
