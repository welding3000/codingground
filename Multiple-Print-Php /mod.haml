@mixin cloud-model-hover() {
  background: rgb(63, 63, 63);
  background: linear-gradient(135deg, rgba(63, 63, 63, 1) 0%, rgba(125, 126, 125, 1) 30%, rgba(125, 126, 125, 1) 70%, rgba(63, 63, 63, 1) 100%);
}


/* ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
.acordion {
  @include cloud();
  cursor: pointer;
}

.acordion:hover {
  @include cloud-hover();
}

#container_model_a {
  @include cloud-model();
  cursor: pointer;
}

#container_model_a:hover {
  @include cloud-model-hover();
}

#cont {
  background: linear-gradient( to top right, rgba(63, 81, 181, 1) 0%, rgba(3, 155, 229, 1) 40%);
  box-shadow: 1px 1px 30px rgba(2, 119, 189, 1);
  padding: 13px;
  margin-top: -16px;
}

#text {
  padding: 8px;
  text-align: center;
  text-transform: capitalize;
  max-width: 600px;
  margin: auto;
  color: white;
  width: 450px;
  background-color: rgba(31, 31, 31, 0.6);
  border-left: 1px solid #e2695f;
  border-radius: 3px;
}

